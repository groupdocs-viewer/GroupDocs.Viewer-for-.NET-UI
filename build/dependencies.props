<Project>
	<PropertyGroup Label="SDK Versions">
		<NetVersions>net6.0;net8.0</NetVersions>
		<NetStandardVersion>netstandard2.0</NetStandardVersion>
		<LangVersion>latest</LangVersion>
	</PropertyGroup>

	<PropertyGroup Label="Package information">
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/groupdocs-viewer/GroupDocs.Viewer-for-.NET-UI</PackageProjectUrl>
		<RepositoryUrl>https://github.com/groupdocs-viewer/GroupDocs.Viewer-for-.NET-UI</RepositoryUrl>
		<Authors>GroupDocs Contributors</Authors>
		<Company>Aspose</Company>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<AllowedOutputExtensionsInPackageBuildOutputFolder>true</AllowedOutputExtensionsInPackageBuildOutputFolder>
	</PropertyGroup>

	<PropertyGroup Label="Package Versions">
		<GroupDocsViewer>25.8.0</GroupDocsViewer>

		<MicrosoftExtensionsHttp>8.0.1</MicrosoftExtensionsHttp>
		<MicrosoftExtensionsDependencyInjectionAbstractions>8.0.2</MicrosoftExtensionsDependencyInjectionAbstractions>
		<MicrosoftExtensionsConfiguration>8.0.0</MicrosoftExtensionsConfiguration>
		<MicrosoftExtensionsConfigurationAbstractions>8.0.0</MicrosoftExtensionsConfigurationAbstractions>
		<MicrosoftExtensionsConfigurationBinder>8.0.2</MicrosoftExtensionsConfigurationBinder>
		<MicrosoftExtensionsCachingMemory>8.0.1</MicrosoftExtensionsCachingMemory>
		<SystemFormatsAsn1>8.0.2</SystemFormatsAsn1>

		<SystemTextJson>8.0.6</SystemTextJson>

		<AzureBlobs>12.25.0</AzureBlobs>
		<AwsSdkS3>4.0.7.2</AwsSdkS3>

		<MicrosoftSourceLinkGithub>8.0.0</MicrosoftSourceLinkGithub>
	</PropertyGroup>

	<PropertyGroup Label="GroupDocs.Viewer UI Package Versions">
		<GroupDocsViewerUI>8.1.1</GroupDocsViewerUI>
		<GroupDocsViewerUIApi>8.1.1</GroupDocsViewerUIApi>
		<GroupDocsViewerUIApiLocalCache>8.1.1</GroupDocsViewerUIApiLocalCache>
		<GroupDocsViewerUIApiInMemoryCache>8.1.1</GroupDocsViewerUIApiInMemoryCache>
		<GroupDocsViewerUIApiLocalStorage>8.1.1</GroupDocsViewerUIApiLocalStorage>
		<GroupDocsViewerUIApiCloudStorage>8.1.1</GroupDocsViewerUIApiCloudStorage>
		<GroupDocsViewerUIApiAzureStorage>8.1.1</GroupDocsViewerUIApiAzureStorage>
		<GroupDocsViewerUIApiAwsS3Storage>8.1.1</GroupDocsViewerUIApiAwsS3Storage>
		<GroupDocsViewerUICore>8.1.1</GroupDocsViewerUICore>
		<GroupDocsViewerUISelfHostApi>8.1.1</GroupDocsViewerUISelfHostApi>
		<GroupDocsViewerUISelfHostApiCrossPlatform>8.1.1</GroupDocsViewerUISelfHostApiCrossPlatform>
		<GroupDocsViewerUICloudApi>8.1.1</GroupDocsViewerUICloudApi>
	</PropertyGroup>

	<PropertyGroup Label="NuGet Common Settings">
		<PackageReleaseNotes>https://github.com/groupdocs-viewer/GroupDocs.Viewer-for-.NET-UI/releases/tag/$(GroupDocsViewerUI)</PackageReleaseNotes>
	</PropertyGroup>
	
</Project>